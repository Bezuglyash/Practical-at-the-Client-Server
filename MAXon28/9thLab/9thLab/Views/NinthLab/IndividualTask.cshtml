@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!doctype html>
<html>
<head>
    <title>Задание | MAXon28</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="~/css/aboutMe.css" />
    <link rel="stylesheet" href="~/css/menu.css" />
    <link rel="stylesheet" href="~/css/individualTask.css" />
</head>
<body>
    <header>
        <div class="div-menu">
            <nav>
                <ul>
                    <li>
                        <a class="notSelectA" style="text-decoration: none;" asp-area="" asp-controller="NinthLab" asp-action="AboutMe">Обо мне</a>
                    </li>
                    <li>
                        <a class="selectA" style="text-decoration: none;" asp-area="" asp-controller="NinthLab" asp-action="IndividualTask">Индивидуальное задание</a>
                    </li>
                    <li>
                        <a class="notSelectA" style="text-decoration: none;" asp-area="" asp-controller="NinthLab" asp-action="Table">Машины</a>
                    </li>
                </ul>
            </nav>
        </div>
    </header>
    <div class="container">
        <div class="card">
            <h1>Введите через запятую, пробел или точку с запятой набор чисел</h1>
            <input id="str" class="inputNumbers" type="text"/>
            <button class="solutionButton" onclick="SolutionClick()">Решить</button>
            <div id="divAnswer" class="divAnswer">
                <p class="answerText">Ответ:</p>
                <p id="answer" class="answer"></p>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/individualTask.js" asp-append-version="true"></script>
</body>
</html>